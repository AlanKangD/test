<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.care.root.member.MemberMapper">
	<resultMap type="com.care.root.member.MemberMapper" id="member">
		<result property="id" column="id" />
		<result property="pw" column="pw" />
	</resultMap>


	<insert id="register">
		insert into test111(id, pw) values(#{id}, #{pw})
	</insert>

</mapper>